<script type="text/template" id="pm2_list_host_container">
    <div class="pm2-host" id="{{server.alias}}">
        <div class="divider"></div>
        <span class="updated">[ UPDATED ]{{updated}}</span>
        <span class="alias">{{server.alias}}</span>
        <table class="table table-hover">
            <thead>
                <th>PID</th>
                <th>PMID</th>
                <th>Name</th>
                <th>Restarts</th>
                <th>Uptime</th>
                <th>Status</th>
                <th>CPU(%)</th>
                <th>Memory(MB)</th>
                <th>Action</th>
            </thead>
        </table>
    </div>
</script>

<script type="text/template" id="my_alert">
<div class="alert my-alert alert-{{class}}">
    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
    {{{content}}}
</div>
</script>
